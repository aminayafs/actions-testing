<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Serialization.NET</id>
    <version>1.4.1</version>
    <authors>Segment, Inc</authors>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <description>Build JsonObject at ease, serializable with Newtonsoft.Json</description>
    <repository type="git" url="https://github.com/segmentio/Serialization.NET" />
    <dependencies>
      <group targetFramework=".NETStandard1.3">
        <dependency id="NETStandard.Library" version="1.6.1" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="13.0.1" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="System.Text.Json" version="7.0.3" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>